---
# vim: set ts=2 sw=2 et ft=yaml :
version: "3.4"

services:
  sshwifty:
    image: niruix/sshwifty:latest
    restart: always
    ports:
      - 8080:8182
    environment:
      SSHWIFTY_SHAREDKEY: admin
      SSHWIFTY_CONFIG: /etc/sshwifty.conf
    volumes:
      - /etc/sshwifty.conf:/etc/sshwifty.conf

volumes: {}

networks: {}

